<?xml version="1.0" encoding="UTF-8"?>
<start>
  <iteration>
 	2
 </iteration>
 <recall>
 	20
 </recall>
 <maxterms>
 	100000
 </maxterms>
 <server>
 	localhost
 </server>
 <temparoryTableName>
 	temp
 </temparoryTableName>
 <SPNameTemplate>
 	UWCSEProcedure
 </SPNameTemplate>
 <mode> 
    <element>
      	:- modeh(*,advisedby(+student,+professor)).
    </element>
    <element>
		:- modeb(*,student(+student)).
	</element>
	<element>
		:- modeb(*,inphase(+student,-phase)).
	</element>

	<element>
		:- modeb(*,yearsinprogram(+student,-years)).
	</element>

	<element>
		:- modeb(*,professor(+professor)).
	</element>
	<element>
		:- modeb(*,hasposition(+professor,-faculty)).
	</element>

	<element>
		:- modeb(*,courselevel(+course,-level)).
	</element>

	<element>
		:- modeb(*,ta(+course,-student,-date)).
	</element>
	<element>
		:- modeb(*,ta(-course,+student,-date)).
	</element>

    <element>
		:- modeb(*,taughtby(+course,-professor,-date)).
    </element>
    <element>
	    :- modeb(*,taughtby(-course,+professor,-date)).
    </element>

	<element>
		:- modeb(*,publicationprofessor(+ref,-professor)).
	</element>
	<element>
		:- modeb(*,publicationprofessor(-ref,+professor)).
	</element>
	<element>
		:- modeb(*,publicationstudent(+ref,-student)).
	</element>
	<element>
		:- modeb(*,publicationstudent(-ref,+student)).
	</element>
 </mode>
 <inlusiondep>
 	<element>
 		:- ind(professor:0,hasposition:0).
 	</element>
 	<element>
 		:- ind(hasposition:0,professor:0).
 	</element>
 	<element>
 		:- ind(student:0,inphase:0).
 	</element>
 	<element>
 		:- ind(inphase:0,student:0).
 	</element>
 	<element>
 		:- ind(student:0,yearsinprogram:0).
 	</element>
 	<element>
 		:- ind(yearsinprogram:0,student:0).
 	</element>
 	<element>
 		:- ind(inphase:0,yearsinprogram:0).
 	</element>
 	<element>
 		:- ind(yearsinprogram:0,inphase:0).
 	</element>
 </inlusiondep>
</start>