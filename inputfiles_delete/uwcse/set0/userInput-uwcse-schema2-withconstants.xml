<?xml version="1.0" encoding="UTF-8"?>
<start>
  <iteration>
 	2
 </iteration>
 <recall>
 	20
 </recall>
 <maxterms>
 	5
 </maxterms>
 <server>
 	localhost
 </server>
 <temparoryTableName>
 	temp
 </temparoryTableName>
 <SPNameTemplate>
 	UWCSEProcedure
 </SPNameTemplate>
 <mode> 
    <element>
      	:- modeh(*,advisedby(+student,+professor)).
    </element>
    <element>
		:- modeb(*,studentinfo(+student,-phase,-years)).
	</element>
	<element>
		:- modeb(*,studentinfo(-student,+phase,-years)).
	</element>
	<element>
		:- modeb(*,studentinfo(-student,-phase,+years)).
	</element>
	<element>
		:- modeb(*,studentinfo(+student,#phase,-years)).
	</element>
	<element>
		:- modeb(*,studentinfo(+student,-phase,#years)).
	</element>
	<element>
		:- modeb(*,studentinfo(+student,#phase,#years)).
	</element>
	
	<element>
		:- modeb(*,publicationstudent(+ref,-student)).
	</element>
	<element>
		:- modeb(*,publicationstudent(-ref,+student)).
	</element>

	<element>
		:- modeb(*,professor(+professor,-faculty)).
	</element>
	<element>
		:- modeb(*,professor(+professor,#faculty)).
	</element>
	<element>
		:- modeb(*,professor(-professor,+faculty)).
	</element>
	
	<element>
		:- modeb(*,publicationprofessor(+ref,-professor)).
	</element>
	<element>
		:- modeb(*,publicationprofessor(-ref,+professor)).
	</element>

	<element>
		:- modeb(*,courselevel(+course,-level)).
	</element>
	<element>
		:- modeb(*,courselevel(+course,#level)).
	</element>
	<element>
		:- modeb(*,courselevel(-course,+level)).
	</element>

	<element>
		:- modeb(*,ta(+course,-student,-date)).
	</element>
	<element>
		:- modeb(*,ta(-course,+student,-date)).
	</element>
	<element>
		:- modeb(*,ta(-course,-student,+date)).
	</element>

    <element>
		:- modeb(*,taughtby(+course,-professor,-date)).
    </element>
    <element>
	    :- modeb(*,taughtby(-course,+professor,-date)).
    </element>
    <element>
	    :- modeb(*,taughtby(-course,-professor,+date)).
    </element>
	
 </mode>
 <inlusiondep>
	<element>
 		:- ind(courselevel:0,ta:0).
 	</element>
 	<element>
 		:- ind(ta:0,courselevel:0).
 	</element>
 	<element>
 		:- ind(courselevel:0,taughtby:0).
 	</element>
 	<element>
 		:- ind(taughtby:0,courselevel:0).
 	</element>
 	
 	<element>
 		:- ind(studentinfo:0,ta:1).
 	</element>
 	<element>
 		:- ind(ta:1,studentinfo:0).
 	</element>
 	
 	<element>
 		:- ind(professor:0,taughtby:1).
 	</element>
 	<element>
 		:- ind(taughtby:1,professor:0).
 	</element>
 	
 	<element>
 		:- ind(studentinfo:0,publicationstudent:1).
 	</element>
 	<element>
 		:- ind(publicationstudent:1,studentinfo:0).
 	</element>
 	<element>
 		:- ind(ta:1,publicationstudent:1).
 	</element>
 	<element>
 		:- ind(publicationstudent:1,ta:1).
 	</element>
 	
 	<element>
 		:- ind(professor:0,publicationprofessor:1).
 	</element>
 	<element>
 		:- ind(publicationprofessor:1,professor:0).
 	</element>
 	<element>
 		:- ind(taughtby:1,publicationprofessor:1).
 	</element>
 	<element>
 		:- ind(publicationprofessor:1,taughtby:1).
 	</element>
 	


 </inlusiondep>
</start>