<?xml version="1.0" encoding="UTF-8"?>
<start>
  <iteration>
 	2
 </iteration>
 <recall>
 	20
 </recall>
 <maxterms>
 	5
 </maxterms>
 <server>
 	localhost
 </server>
 <temparoryTableName>
 	temp
 </temparoryTableName>
 <SPNameTemplate>
 	UWCSEProcedure
 </SPNameTemplate>
 <mode> 
    <element>
      	:- modeh(*,advisedby(+student,+professor)).
    </element>
    
    <element>
		:- modeb(*,studentinfo(+student,#phase,#years)).
	</element>
    <element>
		:- modeb(*,studentinfo(+student,#phase,-years)).
	</element>
	<element>
		:- modeb(*,studentinfo(+student,-phase,#years)).
	</element>
    <element>
		:- modeb(*,studentinfo(+student,-phase,-years)).
	</element>
	<element>
		:- modeb(*,studentinfo(-student,+phase,-years)).
	</element>
	<element>
		:- modeb(*,studentinfo(-student,-phase,+years)).
	</element>
	
	

	<element>
		:- modeb(*,professor(+professor,#faculty)).
	</element>
	<element>
		:- modeb(*,professor(+professor,-faculty)).
	</element>
	<element>
		:- modeb(*,professor(-professor,+faculty)).
	</element>
	
	

	<element>
		:- modeb(*,courselevel(+course,#level)).
	</element>
	<element>
		:- modeb(*,courselevel(+course,-level)).
	</element>
	<element>
		:- modeb(*,courselevel(-course,+level)).
	</element>
	<element>
		:- modeb(*,taughtby(+course,-professor,-date)).
    </element>
    <element>
	    :- modeb(*,taughtby(-course,+professor,-date)).
    </element>
    <element>
	    :- modeb(*,taughtby(-course,-professor,+date)).
    </element>
	<element>
		:- modeb(*,ta(+course,-student,-date)).
	</element>
	<element>
		:- modeb(*,ta(-course,+student,-date)).
	</element>
	<element>
		:- modeb(*,ta(-course,-student,+date)).
	</element>
	
	<element>
			:- modeb(*,publication(+ref,-student)).
		</element>
		<element>
			:- modeb(*,publication(-ref,+student)).
		</element>
		<element>
			:- modeb(*,publication(+ref,-professor)).
		</element>
		<element>
			:- modeb(*,publication(-ref,+professor)).
		</element>

 </mode>
 <inlusiondep>
	
	<element>
 		:- ind(courselevel:0,taughtby:0).
 	</element>
 	<element>
 		:- ind(taughtby:0,courselevel:0).
 	</element>

 </inlusiondep>
</start>